@model IEnumerable<MvcJQuery.Models.UserInfos>
@using MvcJQuery.Models;
<p>@Html.ActionLink("Create New", "Create")</p>
<table>
    <tr>
        <th>编号</th>
        <th>姓名</th>
        <th>性别</th>
        <th>电话</th>
        <th></th>
    </tr>
    @foreach (UserInfos item in Model.ToList())
    {
        <tr>
            <td>@item.Id</td>
            <td>@item.Name</td>
            <td>@item.Gender</td>
            <td>@item.Phone</td>
            <td>
                @Html.ActionLink("Edit", "Edit", new { Id = @item.Id })
                @Html.ActionLink("Delete", "Delete", new { Id = @item.Id })
                @Html.ActionLink("Details", "Details", new { Id = @item.Id })
            </td>
        </tr>
    }
</table>